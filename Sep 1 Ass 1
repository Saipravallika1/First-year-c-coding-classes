#include<stdio.h>
int checkevenodd(int);
int main() {
int n,x,a;
printf("\nEnter the value of n");
scanf("%d",&n);
    x=checkevenodd(n);
    if(n%2==0)
    printf("EVEN");
    else
    printf("ODD");
}
int checkevenodd(int a) 
{
    if(a%2==0)
    return(1);
    else
    return(0);
    

}

    
    
    

    


    
