include <stdio.h>
int max(int, int, int, int);
int main() {
    int a, b, c, d, x;
printf("\nEnter four numbers") ;
scanf("%d%d%d%d",&a,&b,&c,&d);
 x=max(a,b,c,d);
printf("The maximum number is %d",x);
return 0;
}
int max(int m, int n, int o, int p)
{
    
    if(m>n && m>o && m>p)
    return m;
    else if( n>o && n>p)
    return n;
    else if( o>p)
    return o;
    else
    return p;
}
